{
  "presets": ["@babel/preset-react"],
  "plugins": [
    [
      "react-css-modules", {
        "generateScopedName": "[name]__[local]--[hash:base64:5]",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": [
              "postcss-nested"
            ]
          }
        }
      }
    ]
  ]
}
